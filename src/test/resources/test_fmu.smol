        Cont[] fmu2 := simulate("src/test/resources/adder.fmu");
        fmu1 := fmu2;
        return 1;
    end
    Int fail6()
        Cont[in Int rb, out Int rc] fmu1 := simulate("src/test/resources/adder.fmu");
        return 1;
    end
