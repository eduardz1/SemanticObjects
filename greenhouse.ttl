#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#Basilicum
sgh:Basilicum rdf:type owl:Class ;
              rdfs:subClassOf sgh:Plant ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty sgh:hasIdealMoisture ;
                                owl:hasValue 10
                              ] .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#Greenhouse
sgh:Greenhouse rdf:type owl:Class ;
               rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty sgh:hasWaterTank ;
                                                        owl:someValuesFrom sgh:WaterTank
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty sgh:hasLightStrength ;
                                                        owl:someValuesFrom xsd:int
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#Pepper
sgh:Pepper rdf:type owl:Class ;
           rdfs:subClassOf sgh:Plant .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#Plant
sgh:Plant rdf:type owl:Class .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#Pot
sgh:Pot rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty sgh:hasPlant ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass sgh:Plant
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty sgh:hasMoisture ;
                          owl:someValuesFrom xsd:int
                        ] .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#PotGroup
sgh:PotGroup rdf:type owl:Class ;
             rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty sgh:isComposedBy ;
                                                      owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                      owl:onClass sgh:Pot
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty sgh:isWateredBy ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass sgh:Pump
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#Pump
sgh:Pump rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty sgh:hasWaterFlow ;
                           owl:someValuesFrom xsd:int
                         ] .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#Shelf
sgh:Shelf rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty sgh:hasHumidity ;
                            owl:someValuesFrom sgh:humidity
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sgh:hasTemperature ;
                            owl:someValuesFrom xsd:double
                          ] .


###  http://www.semanticweb.org/gianl/ontologies/2023/1/sirius-greenhouse#WaterTank
sgh:WaterTank rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty sgh:hasWaterLevel ;
                                owl:someValuesFrom xsd:int
                              ] .


